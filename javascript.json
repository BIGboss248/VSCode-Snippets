			"\t\tconst xhr = new XMLHttpRequest();",
			"",
			"\t\tthis.url = url;",
			"",
			"\t\txhr.open(\"GET\", url, true);",
			"",
			"\t\theaders.forEach((element) => {",
			"\t\t\txhr.setRequestHeader(element.key, element.value);",
			"\t\t});",
			"",
			"\t\txhr.responseType = responseType;",
			"",
			"\t\txhr.onreadystatechange = function () {",
			"\t\t\tif (xhr.readyState === 4) {",
			"\t\t\t\tif (xhr.status === 200) {",
			"\t\t\t\t\tconsole.log(\"Success:\", xhr.response);",
			"\t\t\t\t} else if (xhr.readyState === 1) {",
