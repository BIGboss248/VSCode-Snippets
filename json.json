	"XRAY proxy outbound settings freedom": {
		"prefix": "!xray outbound settings freedom",
		"isFileTemplate": false,
		"description": "Freedom is an outbound settings protocol that can be used to send (normal) TCP or UDP data to any network.",
		"body": [
			"{",
			"\"domainStrategy\": \"${1|UseIP,UseIPv4,UseIPv6,AsIs|}\",",
			"\"redirect\": \"127.0.0.1:3366\",",
			"\"userLevel\": 0,",
			"\"fragment\": {",
			"  \"packets\": \"tlshello\",",
			"  \"length\": \"100-200\",",
			"  \"interval\": \"10-20\" // ms",
			"},",
			"\"noises\":[",
			"{",
			"  \"type\":\"base64\",",
			"  \"packet\":\"7nQBAAABAAAAAAAABnQtcmluZwZtc2VkZ2UDbmV0AAABAAE=\",",
			"  \"delay\":\"10-16\"",
			"},",
			"{",
			"  \"type\":\"rand\",",
			"  \"packet\":\"10-20\",",
			"  \"delay\":\"10-16\"",
			"},",
			"{",
			"  \"type\":\"str\",",
			"  \"packet\":\"hiGFW\",",
			"  \"delay\":\"10-16\"",
			"}",
			" ],",
			"\"proxyProtocol\": 0",
			" }",
		]
	},
